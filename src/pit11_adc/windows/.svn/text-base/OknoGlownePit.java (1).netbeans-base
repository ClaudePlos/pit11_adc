                       String p48 = tabMSG.getModel().getValueAt(i, 29).toString().replace(",", ".").replace(".00", "");
                       if ( p48.equals("")) p48 = "0";

                       msg = msg + "<P_45>" + p45 + "</P_45>" + '\n';
                       msg = msg + "<P_46>" + p46 + "</P_46>" + '\n';
                       msg = msg + "<P_47>" + p47 + "</P_47>" + '\n';
                       msg = msg + "<P_48>" + p48 + "</P_48>" + '\n';
                       
                       
                       
                       
                       String p66 = tabMSG.getModel().getValueAt(i, 30).toString().replace(",", ".").replace(".00", "");
                       if ( p66.equals("")) p66 = "0";
                       
                       String p68 = tabMSG.getModel().getValueAt(i, 31).toString().replace(",", ".").replace(".00", "");
                       if ( p68.equals("")) p68 = "0";


                       msg = msg + "<P_66>" + p66 + "</P_66>" + '\n';
                       msg = msg + "<P_68>" + p68 + "</P_68>" + '\n';
                       
                       
                       
                       msg = msg + "<P_72>2</P_72>" + '\n';
                   
                       
                       msg = msg + "</PozycjeSzczegolowe>" + '\n';
