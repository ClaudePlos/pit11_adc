    private void butGenXMLActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_butGenXMLActionPerformed
       
        
        try {
                String fileLines = null;
                Writer out2;
                String plikNazwa = null;
                
                for ( int i = 0; i < tabMSG.getRowCount(); i++ )
                {
                       //System.out.println("START");
                           
                       plikNazwa = tabMSG.getModel().getValueAt(i, 5).toString();
                       
                       if ( !tabMSG.getModel().getValueAt(i, 6).toString().equals("") )
                       {
                          plikNazwa =  tabMSG.getModel().getValueAt(i, 6).toString().replace("-", "");
                       }
                       
                       msg =       "<?xml version=\"1.0\" encoding=\"UTF-8\"?>" + '\n';
                       msg = msg + "<Deklaracja xmlns=\"http://crd.gov.pl/wzor/2014/12/08/1887/\">" + '\n';
                       msg = msg + "<Naglowek>" + '\n';
                       msg = msg + "<KodFormularza kodSystemowy=\"PIT-11 (21)\" kodPodatku=\"PIT\" rodzajZobowiazania=\"Z\" wersjaSchemy=\"1-0E\">PIT-11</KodFormularza>" + '\n';
                       msg = msg + "<WariantFormularza>21</WariantFormularza>" + '\n';
                       msg = msg + "<CelZlozenia poz=\"P_6\">1</CelZlozenia>" + '\n'; /// KS - add - 2014-07-04 Jeżeli Korekta to dajemy 2. Zwykłe 1
                       msg = msg + "<Rok>2014</Rok>" + '\n';
                       msg = msg + "<KodUrzedu>" + tabMSG.getModel().getValueAt(i, 3) + "</KodUrzedu>" + '\n';
                       msg = msg + "</Naglowek>" + '\n';
                       msg = msg + "<Podmiot1 rola=\"Płatnik\">" + '\n';;       
                       msg = msg + "<etd:OsobaNiefizyczna xmlns:etd=\"http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2011/06/21/eD/DefinicjeTypy/\">" + '\n';
                       msg = msg + "<etd:NIP>9451979723</etd:NIP>" + '\n';
                       msg = msg + "<etd:PelnaNazwa>HOTELE ADC SPÓŁKA Z OGRANICZONĄ ODPOWIEDZIALNOŚCIĄ</etd:PelnaNazwa>" + '\n';
                       msg = msg + "<etd:REGON>356571563</etd:REGON>" + '\n';
                       msg = msg + "</etd:OsobaNiefizyczna>" + '\n';
                      /* msg = msg + "<AdresZamieszkaniaSiedziby rodzajAdresu=\"RAD\">" + '\n';
                       msg = msg + "<etd:AdresPol>" + '\n';
                       msg = msg + "<etd:KodKraju>PL</etd:KodKraju>" + '\n';
                       msg = msg + "<etd:Wojewodztwo>MAŁOPOLSKIE</etd:Wojewodztwo>" + '\n';
                       msg = msg + "<etd:Powiat>Kraków</etd:Powiat>" + '\n';
                       msg = msg + "<etd:Gmina>Kraków</etd:Gmina>" + '\n';
                       msg = msg + "<etd:Ulica>Żabiniec</etd:Ulica>" + '\n';
                       msg = msg + "<etd:NrDomu>46</etd:NrDomu>" + '\n';
                       msg = msg + "<etd:Miejscowosc>Kraków</etd:Miejscowosc>" + '\n';
                       msg = msg + "<etd:KodPocztowy>31-215</etd:KodPocztowy>" + '\n';
                       msg = msg + "<etd:Poczta>Kraków</etd:Poczta>" + '\n';
                       msg = msg + "</etd:AdresPol>" + '\n';
                       msg = msg + "</AdresZamieszkaniaSiedziby>" + '\n';*/ //adres wywalamy, było potrzebne do 2012
                       msg = msg + "</Podmiot1>" + '\n';
                       msg = msg + "<Podmiot2 rola=\"Podatnik\">" + '\n';
                       msg = msg + "<etd:OsobaFizyczna xmlns:etd=\"http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2011/06/21/eD/DefinicjeTypy/\">" + '\n';
                       
                       if ( tabMSG.getModel().getValueAt(i, 5).toString().equals("") ) // jeżeli brak peselu to wysyłamy nip
                       {
                          msg = msg + "<etd:NIP>" + tabMSG.getModel().getValueAt(i, 6).toString().replace("-", "") + "</etd:NIP>" + '\n'; 
                       }
                       else 
                       {
                          msg = msg + "<etd:PESEL>" + tabMSG.getModel().getValueAt(i, 5) + "</etd:PESEL>" + '\n';  
                       }
                       
                       
                       msg = msg + "<etd:ImiePierwsze>" + tabMSG.getModel().getValueAt(i, 8) + "</etd:ImiePierwsze>" + '\n';
                       msg = msg + "<etd:Nazwisko>" + tabMSG.getModel().getValueAt(i, 7) + "</etd:Nazwisko>" + '\n';
                       msg = msg + "<etd:DataUrodzenia>" + tabMSG.getModel().getValueAt(i, 9) + "</etd:DataUrodzenia>" + '\n';
                       msg = msg + "</etd:OsobaFizyczna>" + '\n';
                       msg = msg + "<etd:AdresZamieszkania xmlns:etd=\"http://crd.gov.pl/xml/schematy/dziedzinowe/mf/2011/06/21/eD/DefinicjeTypy/\" rodzajAdresu=\"RAD\">" + '\n';
                       msg = msg + "<etd:AdresPol>" + '\n';
                       msg = msg + "<etd:KodKraju>PL</etd:KodKraju>" + '\n';
                       msg = msg + "<etd:Wojewodztwo>" + tabMSG.getModel().getValueAt(i, 11) + "</etd:Wojewodztwo>" + '\n';
                       
                       if (tabMSG.getModel().getValueAt(i, 12).equals(""))
                       {
                         JOptionPane.showMessageDialog(null,"Brak powiatu !!! dla: "+tabMSG.getModel().getValueAt(i, 5),"INFO",JOptionPane.WARNING_MESSAGE);  
                       }
                       
                       if (tabMSG.getModel().getValueAt(i, 13).equals(""))
                       {
                         JOptionPane.showMessageDialog(null,"Brak gminy !!! dla: "+tabMSG.getModel().getValueAt(i, 5),"INFO",JOptionPane.WARNING_MESSAGE);  
                       }
                       
                       msg = msg + "<etd:Powiat>" + tabMSG.getModel().getValueAt(i, 12) + "</etd:Powiat>" + '\n';
                       msg = msg + "<etd:Gmina>" + tabMSG.getModel().getValueAt(i, 13) + "</etd:Gmina>" + '\n';
                       
                       
                       if ( !tabMSG.getModel().getValueAt(i, 14).toString().equals("") )
                       msg = msg + "<etd:Ulica>" + tabMSG.getModel().getValueAt(i, 14) + "</etd:Ulica>" + '\n'; 
                       
                       if ( !tabMSG.getModel().getValueAt(i, 15).toString().equals("") )
                       msg = msg + "<etd:NrDomu>" + tabMSG.getModel().getValueAt(i, 15) + "</etd:NrDomu>" + '\n';
                       
                       if ( !tabMSG.getModel().getValueAt(i, 16).toString().equals("") )
                       msg = msg + "<etd:NrLokalu>" + tabMSG.getModel().getValueAt(i, 16) + "</etd:NrLokalu>" + '\n'; 
                       
                       
                       
                       msg = msg + "<etd:Miejscowosc>" + tabMSG.getModel().getValueAt(i, 17) + "</etd:Miejscowosc>" + '\n';
                       msg = msg + "<etd:KodPocztowy>" + tabMSG.getModel().getValueAt(i, 18) + "</etd:KodPocztowy>" + '\n';
                       msg = msg + "<etd:Poczta>" + tabMSG.getModel().getValueAt(i, 19) + "</etd:Poczta>" + '\n';
                       msg = msg + "</etd:AdresPol>" + '\n';
                       msg = msg + "</etd:AdresZamieszkania>" + '\n';
                       msg = msg + "</Podmiot2>" + '\n';
                       msg = msg + "<PozycjeSzczegolowe>" + '\n';
                       
                       msg = msg + "<P_24>" + tabMSG.getModel().getValueAt(i, 20).toString().replace(",", ".").replace(".00", "") + "</P_24>" + '\n';
                       
                       
                       
