                       msg = msg + "</Deklaracja>" + '\n';
                       
                       System.out.println(msg);
                      
                          out2 = new BufferedWriter(new OutputStreamWriter(
                                new FileOutputStream("c:\\pit11_ADC\\out\\pit11_2014_" + plikNazwa + ".xml"),"UTF-8"));
            
                          out2.write( msg );
                          out2.close();
                          msg = "";
                       
                       
                }
                
